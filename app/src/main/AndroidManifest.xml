<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.goodsmanage">

    <uses-permission android:name="android.permission.INTERNET" />

    <meta-data
        android:name="com.example.goodsmanage.OkHttpAppGlideModule"
        android:value="AppGlideModule" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".acitvity.AllOrderActivity"></activity>
        <activity android:name=".acitvity.AllCommentActivity" />
        <activity
            android:name=".RegisterActivity"
            android:label="注册" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".acitvity.AddGoodsActivity"
            android:label="发布商品" />
        <activity
            android:name=".MainActivity"
            android:label="首页" />
        <activity
            android:name=".acitvity.GoodsActivity"
            android:label="商品详情" />
        <activity
            android:name=".acitvity.GoodsListActivity"
            android:label="商品列表" />
    </application>

</manifest>